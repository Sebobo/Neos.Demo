/**
 * Default page-rendering for the Neos.Demo website
 */
print = Neos.Demo:Document.Page {
    head {
        stylesheets.printCss = Neos.Fusion:Tag {
            @position = 'end 10'
            tagName = 'link'
            attributes {
                media = 'all'
                rel = 'stylesheet'
                href = Neos.Fusion:ResourceUri {
                    path = 'resource://Neos.Demo/Public/Styles/Print.css'
                }
            }
        }
    }
}
