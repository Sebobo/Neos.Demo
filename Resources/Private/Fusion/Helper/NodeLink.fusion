prototype(Neos.Demo:Helper.NodeLink) < prototype(Neos.Fusion:Component) {
    node = null
    content = ${this.node ? q(this.node).property('title') : ''}

    renderer = Neos.Fusion:Tag {
        tagName = 'a'
        attributes.href = Neos.Neos:NodeUri {
            node = ${props.node}
        }
        content = ${props.content}
    }
}
